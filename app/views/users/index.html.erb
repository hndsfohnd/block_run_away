<% @users.each do |user|%>
  <div class="user">
    <%= link_to "個人ページへ", user_path(user.id) %>
    <% if current_user.following?(user) %>
      <%= link_to "フォロー解除する", follow_destroy_user_path(user.id) %>
    <% else %>
      <%= link_to "#{user.email} フォローする", follow_create_user_path(user.id) %>
    <% end %>
  </div>

<% end %>



<%= link_to "signout", destroy_user_session_path, method: :delete %>

